  <body>
    <form method="post" action="/createSched">
      Tanggal(yyyy-mm-dd): <input type="text" name="tanggal" placeholder="Tanggal"/> <br/>
      Kegiatan: <input type="text" name="kegiatan" placeholder="Kegiatan"/> <br/>
